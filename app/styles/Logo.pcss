.Logo {
    width: 150px;
    height: 50px;
    display: flex;
    align-items: center;

    a {
        width: 150px;
        display: inline-block;
        text-decoration: none;
        color: #FFF;
        font-family: Montserrat;
        font-size: 23px;
        font-weight: bold;

        .dim {
            opacity: 0.3;
            font-size: 11px;
        }
    }
    
    img {
        width: 150px;
    }

}